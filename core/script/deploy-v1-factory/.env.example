# BasePluginV1Factory Deployment Configuration
# Copy this file to .env and fill in your actual values

# Required Environment Variables
ALGEBRA_FACTORY_ADDRESS=0x1234567890123456789012345678901234567890  # Replace with actual Algebra factory address
REFLEX_ROUTER_ADDRESS=0x0987654321098765432109876543210987654321    # Replace with your ReflexRouter address

# Optional: Farming functionality
FARMING_ADDRESS=0x1111111111111111111111111111111111111111         # Optional: Set farming address during deployment

# Optional: Contract verification
VERIFY_CONTRACT=true                                               # Set to true to verify on Etherscan
ETHERSCAN_API_KEY=your-etherscan-api-key-here                     # Required if VERIFY_CONTRACT=true

# Optional: Custom fee configuration (if not set, uses defaults)
CUSTOM_FEE_CONFIG=false                                           # Set to true to use custom fee parameters below

# Custom fee parameters (only used if CUSTOM_FEE_CONFIG=true)
ALPHA1=2900       # Default: 2900
ALPHA2=12000      # Default: 12000  
BETA1=360         # Default: 360
BETA2=60000       # Default: 60000
GAMMA1=59         # Default: 59
GAMMA2=8500       # Default: 8500
BASE_FEE=100      # Default: 100 (1% in basis points)

# Deployment Network Configuration
RPC_URL=https://polygon-rpc.com                                   # Replace with your RPC URL
PRIVATE_KEY=your-private-key-here                                 # Your deployer private key

# Example Networks:
# Polygon: https://polygon-rpc.com
# Ethereum: https://eth-mainnet.alchemyapi.io/v2/YOUR-API-KEY
# Arbitrum: https://arb1.arbitrum.io/rpc
# BSC: https://bsc-dataseed.binance.org
