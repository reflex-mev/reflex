# Blockchain Integration Test Configuration
# Copy this file to .env and fill in your values

# Required: RPC endpoint URL
# For local Anvil: http://localhost:8545
# For local Hardhat: http://localhost:8545
# For testnets/mainnet: Use your Infura/Alchemy URL
TEST_RPC_URL=http://localhost:8545

# Required: Private key for test account
# Default Anvil account #0 (has 10000 ETH pre-funded on local fork)
# WARNING: Never use this key with real funds!
TEST_PRIVATE_KEY=0xac0974bec39a17e36ba4a6b4d238ff944bacb478cbed5efcae784d7bf4f2ff80

# Required: Deployed Reflex Router contract address
# Get this from your deployment output
REFLEX_ROUTER_ADDRESS=

# Required: Pool address for backrun testing
# Example Uniswap V3 USDC/ETH pool: 0x88e6A0c2dDD26FEEb64F039a2c41296FcB3f5640
TEST_POOL_ADDRESS=

# Optional: Uniswap V3 SwapRouter address
# Mainnet: 0xE592427A0AEce92De3Edee1F18E0157C05861564
SWAP_ROUTER_ADDRESS=0xE592427A0AEce92De3Edee1F18E0157C05861564

# Optional: Execute swap parameters (leave empty to skip swap)
# Token addresses for the swap
TOKEN_IN=
TOKEN_OUT=

# Uniswap V3 fee tier (500 = 0.05%, 3000 = 0.3%, 10000 = 1%)
SWAP_FEE=3000

# Amount to swap in the execute transaction (in token units)
# This amount will also be used for the backrun
SWAP_AMOUNT_IN=0.01

# Minimum output amount (0 for testing, set higher for production)
MIN_AMOUNT_OUT=0

# Optional: Configuration ID for profit splitting
# Leave empty to use default config
TEST_CONFIG_ID=
