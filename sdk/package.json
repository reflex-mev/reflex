{
  "name": "@reflex-mev/sdk",
  "version": "0.0.3",
  "description": "TypeScript SDK for Reflex Router - Execute MEV backruns with ease",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "lint": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint && npm run test && npm run build",
    "version": "npm run format && git add -A src",
    "publish:dry": "npm publish --dry-run",
    "publish:beta": "npm publish --tag beta",
    "publish:latest": "npm publish --tag latest"
  },
  "keywords": [
    "ethereum",
    "defi",
    "mev",
    "arbitrage",
    "backrun",
    "dex",
    "typescript",
    "web3"
  ],
  "author": "Reflex MEV",
  "license": "MIT",
  "dependencies": {
    "ethers": "^6.13.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.8.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.2.0"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/reflex-mev/reflex.git",
    "directory": "sdk"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
